{
  "name": "",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "browserify public/src/js/main.js -o public/bin/base.js",
    "build-css": "node-sass public/src/css/sass/base.scss public/bin/base.css",
    "build-min-js": "browserify public/src/js/main.js | uglifyjs -o public/bin/base.min.js",
    "build-min-css": "node-sass public/src/css/sass/base.scss | cleancss -o public/bin/base.min.css",
    "watch-js": "nodemon -e js -w public/src/js/ -x 'npm run build-js'",
    "watch-css": "nodemon -e scss -w public/src/css/sass -x 'npm run build-css'",
    "watch-server": "nodemon app.js",
    "release-build": "npm run build-min-js & npm run build-min-css",
    "debug-build": "npm run watch-js & npm run watch-css & npm run watch-server"
  },
