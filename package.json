    "express": "~4.13.4",
    "helmet": "~1.3.0",
    "cytoscape": "~2.6.12",
    "cytoscape-panzoom": "~2.2.3",
    "cytoscape-cxtmenu": "~2.6.1",
    "cytoscape-edgehandles": "~2.3.2",
    "filesaverjs": "~0.2.2",
    "cytoscape-qtip": "~2.3.0",
    "qtip2": "~3.0.2",
    "install": "~0.8.1",
    "cytoscape-cose-bilkent": "~1.3.2",
